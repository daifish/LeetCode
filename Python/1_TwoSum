class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        process = {}
        
        for index in range(len(nums)):
            if target - nums[index] in process: 
                return [process[target-nums[index]] + 1, index + 1]
            process[nums[index]] = index
